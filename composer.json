{
    "name": "micro-php-libs/string-formatter",
    "description": "Laravel String Formatter: One helper for formatting string clone from validation",
    "license": "MIT",
    "authors": [
        {
            "name": "Ty Huynh",
            "email": "hongty.huynh@gmail.com",
            "homepage": "https://ty.dbcsoft.net/",
            "role": "Owner"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.3|^8.0",
        "laravel/framework": "^8.41"
    },
    "autoload": {
        "psr-4": {
            "MicroPhpLibs\\StringFormatter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MicroPhpLibs\\StringFormatter\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MicroPhpLibs\\StringFormatter\\StringFormatterServiceProvider"
            ]
        }
    }
}
